<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="br.com.h2capps.bottomexample2.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard">

        <action
            android:id="@+id/goToDashboard2"
            app:destination="@id/navigation_dashboard_2" />

    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard_2"
        android:name="br.com.h2capps.bottomexample2.ui.dashboard2.Dashboard2Fragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard_2" />

</navigation>
