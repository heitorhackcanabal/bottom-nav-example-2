<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="br.com.h2capps.bottomexample2.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/goToHome2"
            app:destination="@id/navigation_home_2" />

    </fragment>

    <fragment
        android:id="@+id/navigation_home_2"
        android:name="br.com.h2capps.bottomexample2.ui.home2.Home2Fragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home_2">

        <action
            android:id="@+id/goToHome3"
            app:destination="@id/navigation_home_3" />

    </fragment>

    <fragment
        android:id="@+id/navigation_home_3"
        android:name="br.com.h2capps.bottomexample2.ui.home3.Home3Fragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home_3" />

</navigation>
